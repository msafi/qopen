{
  "name": "viewineditor",
  "version": "1.4.0",
  "description": "Quickly open GitHub repos and pull-requests in your text editor",
  "main": "bin/vie.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && babel bin -d bin",
    "dev": "tsc --watch",
    "version": "npm run build && git add -A bin",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm version minor"
  },
  "bin": {
    "vie": "bin/vie.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msafi/vie.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          },
          "useBuiltins": true
        }
      ]
    ]
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/msafi/vie/issues"
  },
  "homepage": "https://github.com/msafi/vie#readme",
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "github-api": "^3.0.0",
    "lodash": "^4.17.4",
    "ora": "^1.3.0",
    "shelljs": "^0.7.8",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.9.2",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.25",
    "@types/ora": "^1.3.1",
    "@types/shelljs": "^0.7.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "typescript": "^2.4.2"
  }
}
